pipeline  {
  
agent {label 'Agent_Label'}

   tools {

	maven 'M2_HOME'
     
      }

  stages {

    stage('Checkout') {

	steps{

	checkout scm

		}
	}
    stage('Skiping Test') {

	steps{

     	sh 'mvn install -DskipTests'

	}

	}
    stage('Install') {

	steps {

        sh 'mvn install'

        	}

	}        
    }
}
